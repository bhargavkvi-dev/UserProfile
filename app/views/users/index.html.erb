<h3>Listing Users</h3>



<div class="col-md-9" id="users"><%= render 'users' %></div>


<div id="forms" class="col-md-3">
<div class="panel panel-info">
      <div class="panel-heading">Filters</div>
      <div class="panel-body">
      		
      		<%= form_tag users_path, :method=> "get", :remote => true do %>

                
                <%= label_tag :first_name, "First Name" %>
      			<%= text_field_tag :first_name%><br/>

      			<%= label_tag :last_name, "Last Name" %>
      			<%= text_field_tag :last_name %><br/>

      			<%= label_tag :city, "City" %>
      			<%= text_field_tag :city %><br/>

      			<%= label_tag :gender, "Gender" %>
      			<%= radio_button_tag :gender, 'Male'%>Male&nbsp&nbsp&nbsp
      			<%= radio_button_tag :gender, 'Female' %>Female<br/>

      			<%= label_tag :technology, "Interested Technology" %>
      			<%= select_tag :technology, options_for_select([ "Web Development", "Web Designing","Testing" ]),:include_blank => 'Select Technology' %><br/><br/>

      			

      			<%= submit_tag "Search" %>

      			
			<% end %>

      </div>
</div>
</div>




